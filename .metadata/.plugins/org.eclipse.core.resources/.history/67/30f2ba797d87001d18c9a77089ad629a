package automaton2;

public class Alert {
	public String icon = null;
	public String heading = null;
	public String description = null;
	public String image = null;
	public Float price = null;
	public String linkToProduct = null;
	
	public boolean alertContainsValidProperties(Alert alert) {
		if (alert.icon != null && alert.heading != null && alert.heading != null &&
			alert.heading != null && alert.heading != null) {
			return true;
		}
		return false;
	}
}
